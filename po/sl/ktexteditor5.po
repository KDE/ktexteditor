#: view/kateview.cpp:775
#, kde-format
msgid "Show Non-Printable Spaces"
msgstr "Pokaži nenatisljive presledke"

#: view/kateview.cpp:777
#, kde-format
msgid "Show/hide bounding box around non-printable spaces"
msgstr "Preklopi prikaz okvirja okrog nenatisljivih presledkov"

#: view/kateview.cpp:781
#, kde-format
msgid "Switch to Command Line"
msgstr "Preklopi na ukazno vrstico"

#: view/kateview.cpp:783
#, kde-format
msgid "Show/hide the command line on the bottom of the view."
msgstr "Pokaže/skrije ukazno vrstico na dnu pogleda."

#: view/kateview.cpp:786
#, kde-format
msgid "Input Modes"
msgstr "Vnosni načini"

#: view/kateview.cpp:795
#, kde-format
msgid "Activate/deactivate %1"
msgstr "Preklopi %1"

#: view/kateview.cpp:805
#, kde-format
msgid "&End of Line"
msgstr "Konec vrstice"

#: view/kateview.cpp:807
#, kde-format
msgid "Choose which line endings should be used, when you save the document"
msgstr "Izberite, kateri konci vrstic se uporabljajo, ko shranite dokument"

#: view/kateview.cpp:808
#, kde-format
msgctxt "@item:inmenu End of Line"
msgid "&UNIX"
msgstr "&UNIX"

#: view/kateview.cpp:809
#, kde-format
msgctxt "@item:inmenu End of Line"
msgid "&Windows/DOS"
msgstr "&Windows/DOS"

#: view/kateview.cpp:810
#, kde-format
msgctxt "@item:inmenu End of Line"
msgid "&Macintosh"
msgstr "&Macintosh"

#: view/kateview.cpp:815
#, kde-format
msgid "Add &Byte Order Mark (BOM)"
msgstr "Dodaj oznako zaporedja &bajtov (BOM)"

#: view/kateview.cpp:818
#, kde-format
msgid ""
"Enable/disable adding of byte order marks for UTF-8/UTF-16 encoded files "
"while saving"
msgstr ""
"Omogoči/onemogoči dodajanje oznake zaporedja bajtov med shranjevanjem "
"datotek kodiranih v UTF-8/UTF-16"

#: view/kateview.cpp:822
#, kde-format
msgid "E&ncoding"
msgstr "Kodiranje"

#: view/kateview.cpp:826
#, kde-format
msgid "Look up the first occurrence of a piece of text or regular expression."
msgstr "Poglej prvo pojavitev dela besedila ali regularnega izraza."

#: view/kateview.cpp:830
#, kde-format
msgid "Find Selected"
msgstr "Najdi izbrano"

#: view/kateview.cpp:832
#, kde-format
msgid "Finds next occurrence of selected text."
msgstr "Najde naslednjo pojavitev izbranega besedila."

#: view/kateview.cpp:836
#, kde-format
msgid "Find Selected Backwards"
msgstr "Najdi izbrano nazaj"

#: view/kateview.cpp:838
#, kde-format
msgid "Finds previous occurrence of selected text."
msgstr "Najde prejšnjo pojavitev izbranega besedila."

#: view/kateview.cpp:842
#, kde-format
msgid "Look up the next occurrence of the search phrase."
msgstr "Poglej naslednjo pojavitev iskalne fraze."

#: view/kateview.cpp:846
#, kde-format
msgid "Look up the previous occurrence of the search phrase."
msgstr "Poglej prejšnjo pojavitev iskalne fraze."

#: view/kateview.cpp:850
#, kde-format
msgid ""
"Look up a piece of text or regular expression and replace the result with "
"some given text."
msgstr ""
"Poglej po delu besedila ali regularnega izraza in ga zamenjaj z danim "
"besedilom."

#: view/kateview.cpp:853
#, kde-format
msgid "Automatic Spell Checking"
msgstr "Samodejno preverjanje črkovanja"

#: view/kateview.cpp:854
#, kde-format
msgid "Enable/disable automatic spell checking"
msgstr "Omogoči/onemogoči samodejno preverjanje črkovanja"

#: view/kateview.cpp:860
#, kde-format
msgid "Change Dictionary..."
msgstr "Zamenjaj slovar ..."

#: view/kateview.cpp:861
#, kde-format
msgid "Change the dictionary that is used for spell checking."
msgstr "Zamenja slovar, ki se uporablja za preverjanje črkovanja."

#: view/kateview.cpp:865
#, kde-format
msgid "Clear Dictionary Ranges"
msgstr "Počisti obsege slovarjev"

#: view/kateview.cpp:867
#, kde-format
msgid ""
"Remove all the separate dictionary ranges that were set for spell checking."
msgstr ""
"Odstrani vse ločene slovarske obsege, ki so bili nastavljeni za preverjanje "
"črkovanja."

#: view/kateview.cpp:873
#, kde-format
msgid "Copy as &HTML"
msgstr "Kopiraj kot &HTML"

#: view/kateview.cpp:874
#, kde-format
msgid ""
"Use this command to copy the currently selected text as HTML to the system "
"clipboard."
msgstr ""
"Uporabite ta ukaz za kopiranje trenutno izbranega besedila na sistemsko "
"odložišče v obliki HTML."

#: view/kateview.cpp:878
#, kde-format
msgid "E&xport as HTML..."
msgstr "Izvozi kot HTML..."

#: view/kateview.cpp:880
#, kde-format
msgid ""
"This command allows you to export the current document with all highlighting "
"information into a HTML document."
msgstr ""
"Ta ukaz vam omogoča izvoz trenutnega dokumenta z vsemi podatki o poudarjanju "
"v dokument HTML."

#: view/kateview.cpp:919
#, kde-format
msgid "Move Word Left"
msgstr "Premakni besedo levo"

#: view/kateview.cpp:925
#, kde-format
msgid "Select Character Left"
msgstr "Izberi znak levo"

#: view/kateview.cpp:931
#, kde-format
msgid "Select Word Left"
msgstr "Izberi besedo levo"

#: view/kateview.cpp:937
#, kde-format
msgid "Move Word Right"
msgstr "Premakni besedo desno"

#: view/kateview.cpp:943
#, kde-format
msgid "Select Character Right"
msgstr "Izberi znak desno"

#: view/kateview.cpp:949
#, kde-format
msgid "Select Word Right"
msgstr "Izberi besedo desno"

#: view/kateview.cpp:955
#, kde-format
msgid "Move to Beginning of Line"
msgstr "Premakni se na začetek vrstice"

#: view/kateview.cpp:961
#, kde-format
msgid "Move to Beginning of Document"
msgstr "Premakni se na začetek dokumenta"

#: view/kateview.cpp:967
#, kde-format
msgid "Select to Beginning of Line"
msgstr "Izberi do začetka vrstice"

#: view/kateview.cpp:973
#, kde-format
msgid "Select to Beginning of Document"
msgstr "Izberi do začetka dokumenta"

#: view/kateview.cpp:979
#, kde-format
msgid "Move to End of Line"
msgstr "Premakni do konca vrstice"

#: view/kateview.cpp:985
#, kde-format
msgid "Move to End of Document"
msgstr "Premakni do konca dokumenta"

#: view/kateview.cpp:991
#, kde-format
msgid "Select to End of Line"
msgstr "Izberi do konca vrstice"

#: view/kateview.cpp:997
#, kde-format
msgid "Select to End of Document"
msgstr "Izberi do konca dokumenta"

#: view/kateview.cpp:1003
#, kde-format
msgid "Select to Previous Line"
msgstr "Izberi do prejšnje vrstice"

#: view/kateview.cpp:1009
#, kde-format
msgid "Scroll Line Up"
msgstr "Eno vrstico višje"

#: view/kateview.cpp:1015
#, kde-format
msgid "Move to Next Line"
msgstr "Premakni do naslednje vrstice"

#: view/kateview.cpp:1021
#, kde-format
msgid "Move to Previous Line"
msgstr "Premakni do prejšnje vrstice"

#: view/kateview.cpp:1027
#, kde-format
msgid "Move Cursor Right"
msgstr "Premakni kazalko desno"

#: view/kateview.cpp:1033
#, kde-format
msgid "Move Cursor Left"
msgstr "Premakni kazalko levo"

#: view/kateview.cpp:1039
#, kde-format
msgid "Select to Next Line"
msgstr "Izberi do naslednje vrstice"

#: view/kateview.cpp:1045
#, kde-format
