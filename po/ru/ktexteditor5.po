#, kde-format
msgid "Active Breakpoint"
msgstr "Активная точка останова"

#: syntax/katethemeconfig.cpp:326
#, kde-format
msgid "Reached Breakpoint"
msgstr "Текущая точка останова"

#: syntax/katethemeconfig.cpp:327
#, kde-format
msgid "Disabled Breakpoint"
msgstr "Неактивная точка останова"

#: syntax/katethemeconfig.cpp:328
#, kde-format
msgid "Execution"
msgstr "Выполнение"

#: syntax/katethemeconfig.cpp:329
#, kde-format
msgid "Warning"
msgstr "Предупреждение"

#: syntax/katethemeconfig.cpp:330
#, kde-format
msgid "Error"
msgstr "Ошибка"

#: syntax/katethemeconfig.cpp:332
#, kde-format
msgid ""
"<p>Sets the background color of mark type.</p><p><b>Note</b>: The marker "
"color is displayed lightly because of transparency.</p>"
msgstr ""
"<p>Цвет фона выделенного маркера.</p><p><b>Примечание</b>: маркер "
"показывается более бледным цветом, поскольку является полупрозрачным "
"элементом.</p>"

#: syntax/katethemeconfig.cpp:344
#, kde-format
msgid "Text Templates & Snippets"
msgstr "Шаблоны и фрагменты текста"

#: syntax/katethemeconfig.cpp:349
#, kde-format
msgid "Background"
msgstr "Фон"

